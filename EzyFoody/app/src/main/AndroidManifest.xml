<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ezyfoody">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value=""/>

        <activity
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".DisplayFoodsActivity">

        </activity>

        <activity
            android:name=".DisplaySnacksActivity">

        </activity>

        <activity
            android:name=".MapActivity">

        </activity>

        <activity
            android:name=".OrderFoodsActivity">

        </activity>

        <activity
            android:name=".OrderSnacksActivity">

        </activity>

        <activity
            android:name=".TopUpActivity">

        </activity>

        <activity
            android:name=".MyOrderActivity">

        </activity>

        <activity
            android:name=".OrderCompleteActivity">

        </activity>

        <activity
            android:name=".OrderDrinksActivity">

        </activity>

        <activity
            android:name=".DisplayDrinksActivity">

        </activity>

    </application>

</manifest>